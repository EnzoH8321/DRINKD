{
  "expo": {
    "name": "drinkdapp",
    "slug": "drinkdapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash_icon.png",
    "splash": {
      "image": "./assets/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.lowerbrunii.drinkdapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
